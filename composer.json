{
    "name": "silasjoisten/sonata-multiupload-bundle",
    "description": "A Bundle which allows you to enable Multiupload in Sonata",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Silas Joisten",
            "email": "silasjoisten@gmail.com"
        }
    ],
    "require": {
        "php": "^8.4",
        "oskarstark/symfony-http-responder": "^1.0.0",
        "sonata-project/admin-bundle": "^4.3",
        "sonata-project/media-bundle": "^4.3",
        "symfony/twig-bridge": "^4.4 || ^5.4 || ^6.4 || ^7.0",
        "twig/twig": "^2.12 || ^3.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "matthiasnoback/symfony-dependency-injection-test": "^4.3",
        "phpunit/phpunit": "^10.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/webpack-encore-bundle": "^2.0"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.4.4"
        }
    },
    "autoload": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SilasJoisten\\Sonata\\MultiUploadBundle\\Tests\\": "tests/"
        }
    }
}
